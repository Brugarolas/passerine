# not turing complete (yet!)
START = block END

block = (expr SEP). expr
expr  = '{' + block + '}'
      | '(' + expr. + ')'
      | op
      | literal

op = assignment

assignment = SYMBOL '=' expr
           | function

function = SYMBOL '->' expr

literal = SYMBOL
        | number
        | STRING
        | BOOLEAN

number = REAL
